<body>
  		<div class="containerform">
  			<h2>YOUR POSTS: </h2> 
  			<div class="row">
    		<div class="col-sm-6">
			<div class="card">
			  <div class="card-block">
			  	{{#each item_name}}
			    <h4 class="card-title">{{this.item_name}}</h4>
			    <h6 class="card-subtitle mb-2 text-muted">{{this.category}}</h6>
			    <p class="card-text">Description: {{this.description}}<br>
			    Fee: ${{this.fee}}<br>
			    Email: {{this.finders_email}}</p>
			    {{/each}}
			  </div>
			</div>
			</div>
			</div>
  		</div>
</body>


